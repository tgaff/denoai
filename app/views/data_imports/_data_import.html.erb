<div id="<%= dom_id data_import %>">
  <% if action_name != "show" %>
    <%= link_to "Show this data import", data_import, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this data import", edit_data_import_path(data_import), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
